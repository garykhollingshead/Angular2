{
  "name": "angular2",
  "version": "0.0.1",
  "description": "Angular 2",
  "main": "build/modules/main.module.js",
  "scripts": {
    "cleanup": "rimraf src/app/**/*.ngfactory.ts src/app/**/*.ngstyle.ts src/app/**/*.ngsummary.json build dist",
    "explorer": "source-map-explorer ./dist/main.bundle.js ./dist/main.bundle.js.map",
    "ngc": "ngc",
    "serve": "http-server ./dist -a 0.0.0.0 -p 9000",
    "start": "npm run cleanup && npm run ngc && concurrently -r \"webpack-dev-server --https --config webpack.aot.config.js\"",
    "start-prod": "npm run ngc && npm run build && npm run serve",
    "build": "npm run cleanup && npm run ngc && webpack --config webpack.aot.config.js -p",
    "deploy": "gh-pages -d dist",
    "aot": "npm run cleanup && npm run ngc && concurrently -r \"webpack-dev-server --config webpack.aot.config.js\" \"node ./bin/ngc-watch.js\""
  },
  "dependencies": {
    "@angular/common": "^2.4.4",
    "@angular/compiler": "^2.4.4",
    "@angular/core": "^2.4.4",
    "@angular/forms": "^2.4.5",
    "@angular/http": "^2.4.5",
    "@angular/platform-browser": "^2.4.4",
    "@angular/platform-browser-dynamic": "^2.4.4",
    "@angular/router": "^3.4.4",
    "@angular/tsc-wrapped": "^0.5.1",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.0.3",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.4",
    "@angular/platform-server": "^2.4.4",
    "@ngtools/webpack": "^1.2.6",
    "@types/node": "^7.0.0",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "chokidar": "^1.6.1",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "gh-pages": "^0.12.0",
    "http-server": "^0.9.0",
    "node-sass": "^4.5.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "source-map-explorer": "^1.3.3",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.0.10",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
